<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="SwifRefLoader"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <requiredProperties>
    <requiredProperty key="developerName">
      <defaultValue>Nicolas Tardy</defaultValue>
    </requiredProperty>
    <requiredProperty key="developerEmail">
      <defaultValue>ntardy@si2m.fr</defaultValue>
    </requiredProperty>
    <requiredProperty key="distributionManagementUrl">
      <defaultValue>http://vliql-usil.si2m.tec:8081/nexus/content/repositories</defaultValue>
